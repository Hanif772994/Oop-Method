   "source": [
    "#inheritance\n",
    "class animal:\n",
    "    def said(self):\n",
    "        print(\"the cow mow\")\n",
    "class smallcow(animal):\n",
    "    def speak(self):\n",
    "        print(\"the smallcow mow slowly\")\n",
    "class dog(smallcow):\n",
    "    def brak(self):\n",
    "         print(\"dog can brak Well\")\n",
    "\n",
    "d=dog()\n",
    "d.said()\n",
    "d.speak()\n",
    "d.brak()  "
   ]
  }
