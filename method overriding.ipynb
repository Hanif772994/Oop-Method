   "source": [
    "# method overriding\n",
    "class animal:\n",
    "    def makesound(self):\n",
    "        print(\"Animal makes a sound\")\n",
    "class dog (animal):\n",
    "    def makesound(self):\n",
    "        print(\"dog barks\") \n",
    "        return super().makesound()\n",
    "d1=dog()\n",
    "d1.makesound()  "
   ]
